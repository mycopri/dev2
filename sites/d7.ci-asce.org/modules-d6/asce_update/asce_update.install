<?php

/**
 * Implementation of hook_schema().
 */
function asce_update_schema() {
  $schema = array();
  return $schema;
}

/**
 * Implementation of hook_uninstall().
 */
function asce_update_uninstall() {
  // Remove all database tables.
  drupal_uninstall_schema('asce_update');
}

function asce_update_install() {
  db_query("UPDATE {system} SET weight = 10 WHERE name = 'asce_update'");
}

